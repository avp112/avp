.clear { /* generic container (i.e. div) for floating buttons */
    overflow: hidden;
    width: 100%;
}

a.buttonn {
    background: transparent url(images/knopf_0_ungedrueckt.gif) no-repeat top right;
    font: normal 20px arial, sans-serif;
	color: #FFF!important;
	text-decoration: none;
    display: block;
    float: left;
    height: 34px;
    margin-right: 0px;
    padding-right: 18px; /* sliding doors padding */
}

a.buttonn:hover {
color: #000!important;
text-decoration: none;
}

a.buttonn span {
    background: transparent url(images/knopf_0_ungedrueckt.gif) no-repeat;
    display: block;
    line-height: 34px;
    padding: 0px 0 0px 18px;
}

a.buttonn:active {
    background-position: bottom right;
    outline: none; /* hide dotted outline in Firefox */
}

a.buttonn:active span {
    background-position: bottom left;
	color: #FFF!important;
    padding: 0px 0 0px 18px; /* push text down 1px */
}

